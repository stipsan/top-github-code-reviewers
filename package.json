      "git add"
    ]
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "eslint": "3.19.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-config-react-app": "1.0.5",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.1",
    "eslint-plugin-react": "7.0.1",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "lint-staged": "4.0.2",
    "prettier": "1.5.3",
    "react-test-renderer": "15.6.1"
  }
